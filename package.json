{
  "name": "ember-react-components",
  "version": "3.0.2",
  "description": "React component support in Ember",
  "keywords": [
    "ember-addon",
    "decorator",
    "react"
  ],
  "repository": "https://github.com/alexlafroscia/ember-react-components",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://alexlafroscia.com/ember-react-components",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --cache .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "7.14.3",
    "ember-auto-import": "1.11.3",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-babel-plugin-helpers": "^1.1.1",
    "ember-cli-htmlbars": "^4.3.1",
    "ember-cli-version-checker": "5.1.2",
    "is-object": "^1.0.2",
    "is-plain-obj": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@ember/edition-utils": "^1.2.0",
    "@ember/optional-features": "^1.3.0",
    "@ember/test-helpers": "^2.2.6",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.18.0",
    "ember-cli-addon-docs": "^3.0.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-data": "^3.27.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.4",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-sinon-qunit": "^6.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-ember": "^10.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "qunit-dom": "^1.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sinon": "^11.1.1",
    "standard-version": "^8.0.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
