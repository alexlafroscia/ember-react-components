{
  "name": "ember-cli-react",
  "version": "1.0.2",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://alexlafroscia.com/ember-cli-react",
  "repository": "https://github.com/alexlafroscia/ember-cli-react",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint --ext js --ext ts addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@ember-decorators/babel-transforms": "^2.0.0",
    "@types/ember": "^2.8.25",
    "@types/ember-qunit": "^3.0.2",
    "@types/ember-test-helpers": "^0.7.1",
    "@types/ember-testing-helpers": "^0.0.3",
    "@types/handlebars": "^4.0.38",
    "@types/qunit": "^2.5.1",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/rsvp": "^4.0.1",
    "babel-eslint": "^8.2.2",
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~3.0.2",
    "ember-cli-addon-docs": "^0.3.0",
    "ember-cli-addon-docs-yuidoc": "^0.1.1",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-testdouble": "^0.1.3",
    "ember-cli-typescript": "^1.3.2",
    "ember-cli-uglify": "^2.0.0",
    "ember-decorators": "^2.0.0-beta.2",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-metrics": "^0.12.1",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^1.0.0-rc.8",
    "loader.js": "^4.2.3",
    "prettier": "^1.11.1",
    "qunit-dom": "^0.5.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "standard-version": "^4.4.0",
    "testdouble": "^2.0.0",
    "typescript": "^2.9.1",
    "typescript-eslint-parser": "^16.0.0"
  },
  "engines": {
    "node": "6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
